{
  "name": "nsg-cloud-storage-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "serverless-webpack": "^5.3.1",
    "uuid": "^3.3.3",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.570.0",
    "serverless": "^1.57.0"
  },
  "scripts": {
    "sls-deploy": "cd $INIT_CWD && aws-okta exec nonprod -- npx sls deploy",
    "sls-remove": "cd $INIT_CWD && aws-okta exec nonprod -- npx sls remove",
    "deploy-api": "cd api && npm run sls-deploy",
    "build": "webpack",
    "deploy-storage": "cd storage && npm run sls-deploy",
    "remove-api": "cd api && npm run sls-remove",
    "remove-storage": "cd storage && npm run sls-remove",
    "lambda": "cd api && aws-okta exec nonprod -- npx sls invoke local --env DYNAMODB_TABLE='nsg-acl-storage-dev'"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.nordstrom.com:nsg/network-mgmt-stack.git"
  },
